{
  "name": "nuxt-element-dashboard",
  "version": "2.2.0",
  "description": "Nuxt2 + element UI project",
  "author": "levy <levy9527@qq.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "element-ui",
    "nuxt",
    "dashboard"
  ],
  "engines": {
    "npm": ">= 5.2.0"
  },
  "scripts": {
    "mock": "cross-env MODE=mock nuxt",
    "dev": "cross-env HOST=0.0.0.0 MODE=dev PORT=3000 nuxt",
    "mock:nologin": "cross-env MODE=mock NO_LOGIN=1 nuxt",
    "dev:nologin": "cross-env MODE=dev NO_LOGIN=1 nuxt",
    "build": "cross-env MODE=prod nuxt build",
    "build:docker": "nuxt build",
    "start": "cross-env HOST=0.0.0.0 PORT=3333 MODE=prod nuxt start",
    "start:docker": "nuxt start",
    "generate": "cross-env MODE=prod nuxt generate",
    "deploy": "now --npm --public"
  },
  "dependencies": {
    "@femessage/el-data-tree": "^0.4.0",
    "@femessage/el-form-renderer": "^1.6.0",
    "@femessage/el-select-area": "^1.0.0",
    "@femessage/upload-to-ali": "^1.2.0",
    "@femessage/v-editor": "^1.0.2",
    "@femessage/v-qrcode": "^1.0.0",
    "@levy9527/pattern-js": "^1.0.0",
    "@nuxtjs/axios": "^5.0.1",
    "@nuxtjs/dotenv": "^1.3.0",
    "dayjs": "^1.7.4",
    "dotenv": "^6.0.0",
    "downloadjs": "^1.4.7",
    "echarts": "^4.1.0",
    "el-data-table": "^1.6.0",
    "element-ui": "^2.10.1",
    "file-saver": "^2.0.1",
    "js-cookie": "^2.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.set": "^4.3.2",
    "lodash.throttle": "^4.1.1",
    "nuxt": "2.9.2",
    "v-charts": "^1.19.0",
    "vuedraggable": "^2.23.0",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "@nuxtjs/style-resources": "^0.1.2",
    "babel-eslint": "^8.2.1",
    "babel-plugin-component": "^1.1.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.0",
    "stylelint": "^9.10.0",
    "stylelint-config-standard": "^18.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "git add"
    ]
  }
}
